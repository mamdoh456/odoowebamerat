"                </td>\n"
"            </tr>\n"
"            % endif\n"
"            <tr>\n"
"                <td style=\"text-align: center;font-size:14px;border-collapse: separate;margin-top:0px;\">\n"
"                % if object.company_id.displayaddress:\n"
"                <strong><a href=\"${object.company_id.imagemaplink}\" target=\"_blank\">${object.company_id.displayaddress or ''}</a></strong>\n"
"                % endif\n"
"                </td>\n"
"            </tr>\n"
"            <tr>\n"
"                <td style=\"text-align: center;\">\n"
"                % if object.company_id.imagemap_file:\n"
"                <a href=\"${object.company_id.imagemaplink}\" target=\"_blank\"><img border=\"0\" alt=\"See Map\" src=\"data:image/png;base64,${object.company_id.get_imagemap_file()}\" style=\"max-width: 600px;\"/></a>\n"
"                % endif\n"
"                </td>\n"
"            </tr>\n"
"            <tr>\n"
"                <td style=\"text-align: center;\">\n"
"                % if object.company_id.waze or object.company_id.gmaps :\n"
